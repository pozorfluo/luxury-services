<form method="post" action="{{ path('admin_note_delete', {'id': admin_note.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ admin_note.id) }}">
    <button type="submit" class="btn btn-danger btn-sm btn-block">
  {{ button_label|default('Delete') }}
</button>
</form>
