<!DOCTYPE html>
<html lang="fr">
  <head>
    <meta charset="UTF-8" />
    <meta
      name="viewport"
      content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0"
    />
    <meta http-equiv="X-UA-Compatible" content="ie=edge" />

    <!--Import Google Icon Font-->
    <link
      href="https://fonts.googleapis.com/icon?family=Material+Icons"
      rel="stylesheet"
    />
    <!--Import bootstrap .css-->
    <link
      rel="stylesheet"
      href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css"
      integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk"
      crossorigin="anonymous"
    />
    {% block stylesheets %}{% endblock %}

    <title>
      {% block title %} Luxury Services {% endblock %}
    </title>
  </head>

  <body class="pt-4">
    <details>
      <summary>
        {{ dump(app.request.get('_controller')) }}
      </summary>
      {{ dump(app.request) }}
    </details>
    <details>
      <summary>
        routes
      </summary>
    </details>
    {% block nav %}
    <nav class="navbar navbar-expand-lg  fixed-top navbar-dark text-secondary bg-primary">
      <a class="navbar-brand" href="#">Devbar</a>
      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarNav">
        <ul class="navbar-nav">
          <li>

          </li>
          {{ render(controller('App\\Controller\\AdminNoteController::dumpRoutes')) }}
          <li class="nav-item active">
            <a
                href="{{ path('user_index') }}"
                class="nav-link"
                >user</a
              >
          </li>
          <li class="nav-item active">
            <a
                href="{{ path('client_index') }}"
                class="nav-link"
                >client</a
              >
          </li>
          <li class="nav-item active">
            <a
                href="{{ path('job_sector_index') }}"
                class="nav-link"
                >job_sector</a
              >
          </li>
          <li class="nav-item active">
            <a
                href="{{ path('job_index') }}"
                class="nav-link"
                >job</a
              >
          </li>
          <li class="nav-item active">
            <a
                href="{{ path('profile_index') }}"
                class="nav-link"
                >profile</a
              >
          </li>
          <li class="nav-item active">
            <a
            href="{{ path('application_index') }}"
            class="nav-link"
            >application</a
          >
          </li>
          <li class="nav-item active">
            <a
            href="{{ path('admin_note_index') }}"
            class="nav-link"
            >admin_note</a
          >
          </li>
          <li class="nav-item active">
            <a
            href="{{ path('profile_show_user') }}"
            class="nav-link"
            >user profile</a
          >
          </li>
          <li class="nav-item active">
            <a
            href="{{ path('app_login') }}"
            class="nav-link"
            >
              <span class="material-icons">
                login
              </span>
            </a
          >
          </li>
          <li class="nav-item active">
            <a
            href="{{ path('app_logout') }}"
            class="nav-link"
            >
              <span class="material-icons">
                close
              </span>
            </a
          >
          </li>
        </ul>
      </div>
    </nav>

    {% endblock %}
    <div class="container pt-5">

      {{ include('_flash.html.twig')}}

      {% block body %}{% endblock %}
    </div>
    {% block footer %} {% endblock %}
    
    <!--Import bootstrap .js -->
    <script
      src="https://code.jquery.com/jquery-3.5.1.slim.min.js"
      integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj"
      crossorigin="anonymous"
    ></script>
    <script
      src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js"
      integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo"
      crossorigin="anonymous"
    ></script>
    <script
      src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.min.js"
      integrity="sha384-OgVRvuATP1z7JjHLkuOU7Xw704+h835Lr+6QL9UvYjZE3Ipu6Tp75j7Bh/kR0JKI"
      crossorigin="anonymous"
    ></script>
    {% block javascripts %}{% endblock %}
  </body>
</html>
