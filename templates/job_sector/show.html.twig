{% extends 'base.html.twig' %}

{% block title %}JobSector{% endblock %}

{% block body %}
    <h1>JobSector</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ job_sector.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ job_sector.name }}</td>
            </tr>
        </tbody>
    </table>

    <a 
      href="{{ path('job_sector_index') }}" 
      class="btn btn-light btn-lg btn-block"
    >
      back to list
    </a>

    <a 
      href="{{ path('job_sector_edit', {'id': job_sector.id}) }}"
      class="btn btn-secondary btn-sm btn-block"
    >
      edit
    </a>

    {{ include('job_sector/_delete_form.html.twig') }}
{% endblock %}
