<form method="post" action="{{ path('job_sector_delete', {'id': job_sector.id}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ job_sector.id) }}">
    <button type="submit" class="btn btn-danger">
  {{ button_label|default('Delete') }}
</button>
</form>
